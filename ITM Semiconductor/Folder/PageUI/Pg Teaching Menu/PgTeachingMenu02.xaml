<Page x:Class="ITM_Semiconductor.PgTeachingMenu02"
       xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ITM_Semiconductor"
      mc:Ignorable="d" 
       d:DesignHeight="790" d:DesignWidth="1300"
      Title="PgTeachingMenu02">

    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="200"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="1">
            <Grid.Resources>
                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="#dd222222" />
                    <Setter Property="Padding" Value="5,0" />
                    <Setter Property="VerticalContentAlignment" Value="Center" />
                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                    <Setter Property="FontSize" Value="24" />
                </Style>
                <Style TargetType="Button" BasedOn="{StaticResource sButton}">
                    <Setter Property="FontSize" Value="24" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Margin" Value="0,5,5,5" />
                    <Setter Property="Background" Value="White" />
                    <Setter Property="BorderThickness" Value="1"/>
                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="22" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Foreground" Value="Black"/>
                </Style>
            </Grid.Resources>
            <Border>
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="#6787c0" Offset="0"/>
                        <GradientStop Color="#879ec8" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
                <Grid>
                    <Canvas>
                        <Polygon Points="0,20,130,80,0,170" Fill="#879ec8"/>
                        <Polygon Points="100,400,200,370,180,470" Fill="#879ec8"/>
                        <Ellipse Margin="250 450 0 0" Width="40" Height="40" Fill="#879ec8"/>
                        <Ellipse Margin="50 400 0 0" Width="40" Height="40" Fill="#879ec8"/>
                    </Canvas>
                    <StackPanel VerticalAlignment="Bottom">
                        <TextBlock Text="ITM" Style="{StaticResource titleText}"/>
                        <TextBlock Text="Semiconductor" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0" FontWeight="Bold"/>
                        <TextBlock Text="AUTOMATION TEAM" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" >
                        <Label  FontWeight="Bold" Padding="0 20 0 0" Foreground="White" FontSize="27" HorizontalAlignment="Center">
                            <Label.Effect>
                                <DropShadowEffect Color="Black" Direction="320" ShadowDepth="5" Opacity="0.3"/>
                            </Label.Effect> System Menu
                        </Label>
                    </StackPanel>
                    <StackPanel Orientation="Vertical"  HorizontalAlignment="Center" Margin="0 100 0 0" >
                        <Button x:Name="btSetting1"  Height="60" Width="215" >
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Padding="10" Text="MAGAZINE"  Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>
                        <Button x:Name="btSetting2"  Height="60" Width="215" >
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Padding="10" Text="CHANNEL 1"  Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                        <Button x:Name="btSetting3" Background="LightGreen"  Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0 5 5 5" BorderThickness="0">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Padding="10" Text="CHANNEL 2" Foreground="Black" Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>
                        <Button x:Name="btSetting4"   Height="60" Width="215" >
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Padding="10" Text="MATRIX/PALLET"  Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
        <Grid Grid.Column="0" >
            <Grid.Resources>

                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="#dd222222" />
                    <Setter Property="Padding" Value="5,0" />
                    <Setter Property="VerticalContentAlignment" Value="Center" />
                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                    <Setter Property="FontSize" Value="20" />
                </Style>
                <Style TargetType="Button" BasedOn="{StaticResource ssButton}">
                    <Setter Property="FontSize" Value="14"/>
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="Margin" Value="9"/>
                    <Setter Property="Background" Value="White"/>
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="#6787c0"/>
                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="20" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Foreground" Value="Black"/>

                </Style>

            </Grid.Resources>
            <Grid Grid.Column="0" Margin="5,0,2,2" Name="gridLeft">
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.7*"></RowDefinition>
                    <RowDefinition Height="2*"></RowDefinition>
                    <RowDefinition Height="3.5*"></RowDefinition>
                    <RowDefinition Height="5*"></RowDefinition>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#bbbbbb" MinHeight="60">
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Background="Orange">
                            <Label Grid.Column="0" Foreground="#EEEEEE" FontSize="25" FontWeight="Bold"
                               Margin="20,0,0,0" VerticalContentAlignment="Center">
                                MANUAL TEACHING [CH2]
                            </Label>
                        </Grid>
                        <Button Grid.Column="2" x:Name="btSave1" Margin="5 5 5 5">
                            <StackPanel Orientation="Horizontal">
                                <Image Margin="5" Source="/Folder/PageUI/Icon/save.png" />
                                <TextBlock VerticalAlignment="Center" Margin="10,0,10,0"
             FontSize="15" FontWeight="Bold">SAVE</TextBlock>
                            </StackPanel>
                        </Button>

                    </Grid>
                    <!--<TextBlock Foreground="Orange" FontSize="24" Margin="20,0,0,0" VerticalAlignment="Center">
                    Base Teaching
                </TextBlock>-->
                </Border>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.4*"></ColumnDefinition>
                        <ColumnDefinition Width="0.3*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="0.2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" FontSize="13" FontWeight="Bold"  >[SERVO POSITION]</Label>
                        <Grid Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1.5*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">DATA</Label>

                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="1" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="2,0,1,1" FontWeight="Bold">AXIS#02 Y</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="1" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">POSITION CURRENT</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="2" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">POSITION ACTUAL</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="3" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">POSITION DIFERENCE</Label>
                            <Grid Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionDataX1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">000.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="1" Margin="2,2,2,2" Background="#222222" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionDataZ1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">000.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="ActualPositionX1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">000.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="2" Margin="2,2,2,2" Background="#222222" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="ActualPositionZ1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">000.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="DeferencePositionX1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay3}">000.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="3" Margin="2,2,2,2" Background="#222222" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="DeferencePositionZ1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay3}">000.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                        </Grid>

                    </Grid>
                    <Grid Grid.Column="1" Margin="10,0,0,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" FontSize="13" FontWeight="Bold"  >[SERVO STATUS]</Label>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="0.13*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border>
                                <Grid Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="2*"></RowDefinition>
                                        <RowDefinition Height="1*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="1,1,1,1" Background="#BBBBBB">
                                        <Label FontSize="20" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold">AXIS#02</Label>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Label Margin="1,1,1,1" Grid.Column="0" Style="{StaticResource LabelLime}">S</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="1" Style="{StaticResource LabelBlue}">H</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="2" Style="{StaticResource LabelBlack}">A</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="3" Style="{StaticResource LabelBlack}">[+]</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="4" Style="{StaticResource LabelBlack}">[-]</Label>
                                    </Grid>
                                </Grid>
                            </Border>
                        </Grid>

                    </Grid>
                    <Grid Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" FontSize="13" FontWeight="Bold"  >[ROBOT POSITION]</Label>
                        <Grid Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1.2*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="1" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">AXIS X</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">DATA</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="1" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">POSITION CURRENT</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="2" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">SPEED CURRENT</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="3" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">SPEED ACTIVE</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="2" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">AXIS Y</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="3" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">AXIS Z</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="4" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">AXIS U</Label>

                            <Grid Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_X" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">000.000</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtSpeedCurrentRobot" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">000</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">%</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <xctk:IntegerUpDown Value="100" Minimum="0" Maximum="100" Foreground="White" Background="#454343" x:Name="txtSetSpeedRobot2" Width="50" Margin="2,2,2,2" />
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">%</Label>
                                </StackPanel>
                            </Grid>

                            <Grid Grid.Column="2" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_Y" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">000.000</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="3" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_Z" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">000.000</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="4" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_R" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">000.000</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="1">
                            <Button Grid.Row="1" Margin="15,10,10,10" x:Name="btReset1">
                                <StackPanel Orientation="Vertical">
                                    <Image Height="60" Width="60" Source="/Folder/PageUI/Icon/alarm.png" Stretch="Fill"/>
                                    <TextBlock  VerticalAlignment="Center" Padding="0,0,0,0" FontSize="20" Foreground="Blue" FontFamily="Times New Roman (Headings)">RESET</TextBlock>
                                </StackPanel>
                            </Button>
                        </Grid>
                    </Grid>
                </Grid>
                <TabControl Name="TabControl1" Grid.Row="2">
                    <TabItem Height="30" Header="AXIS#02 Y TRAY MOV">
                        <Grid>
                            <Border Grid.Row="2" Margin="5,5,5,5" BorderBrush="Silver" Background="#FFFFFF" BorderThickness="1" CornerRadius="2,2,2,2">
                                <Grid Grid.Row="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*"></RowDefinition>
                                        <RowDefinition Height="1.5*"></RowDefinition>
                                        <RowDefinition Height="2*"></RowDefinition>

                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Margin="10,2,10,2" BorderThickness="1" BorderBrush="#222222">
                                        <Label Margin="2,2,2,2" BorderThickness="0" Background="Orange" FontWeight="Bold" Foreground="White" HorizontalContentAlignment="Center">Select the location you want to move(이동하려는 위치를 선택하십요)</Label>
                                    </Border>
                                    <Border Grid.Row="1" Margin="10,2,10,2" BorderThickness="1" Background="LightYellow" CornerRadius="2,2,2,2">
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="0*"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Button Name="btnBendingPos" Grid.Column="1" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">INPUT Pos.</Button>
                                            <Button Name="btnReadyPos" Grid.Column="2" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">CENTER Pos.</Button>
                                            <Button Name="btnQrPos" Grid.Column="3" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">OUTPUT Pos.</Button>
                                            <Button Name="btnQrScap" Grid.Column="4" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">CENTER FEED Pos.2</Button>
                                        </Grid>
                                    </Border>
                                    <Grid Grid.Row="2" Margin="10,2,10,2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="0*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Label Margin="1,1,1,1" Grid.Row="0" Grid.Column="0" Style="{StaticResource Labelteaching}">POSITION [mm]</Label>
                                        <Label Margin="1,1,1,1" Grid.Row="1" Grid.Column="0"  Style="{StaticResource Labelteaching}">SPEED [mm/s]</Label>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="0" Grid.Column="1">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtBendingPosAxisX"  FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000.00</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="0" Grid.Column="2">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtReadyPosAxisX" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000.00</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="0" Grid.Column="3">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtQrPosAxisX" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000.00</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="0" Grid.Column="4">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtQrScapPosAxisX" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000.00</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="0" Grid.Column="6">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtMatingPosAxisX" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000.00</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="1" Grid.Column="1">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtBendingPosAxisZ" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="1" Grid.Column="2">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtReadyPosAxisZ" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="1" Grid.Column="3">
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtQrPosAxisZ" FontSize="22" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}">000</TextBox>
                                        </Grid>
                                        <Grid Background="#222222" Margin="5,1,5,1" Grid.Row="1" Grid.Column="6">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="126*"/>
                                                <ColumnDefinition Width="17*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBox  PreviewTextInput="NumberValidationTextBox" Name="txtMatingPosAxisZ" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay}" Grid.ColumnSpan="2"></TextBox>
                                        </Grid>
                                    </Grid>
