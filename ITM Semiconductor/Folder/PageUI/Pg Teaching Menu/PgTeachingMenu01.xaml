<Page x:Class="ITM_Semiconductor.PgTeachingMenu01"
      xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ITM_Semiconductor"
      mc:Ignorable="d" 
     d:DesignHeight="840" d:DesignWidth="1300"
      Title="PgTeachingMenu01">

    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition ></ColumnDefinition>
            <ColumnDefinition Width="200"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="1">
            <Grid.Resources>

                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="#dd222222" />
                    <Setter Property="Padding" Value="5,0" />
                    <Setter Property="VerticalContentAlignment" Value="Center" />
                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                    <Setter Property="FontSize" Value="24" />
                </Style>
                <Style TargetType="Button" BasedOn="{StaticResource sButton}">
                    <Setter Property="FontSize" Value="24" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Margin" Value="0,5,5,5" />
                    <Setter Property="Background" Value="White" />
                    <Setter Property="BorderThickness" Value="0"/>

                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="22" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Foreground" Value="Black"/>

                </Style>

            </Grid.Resources>
            <Border>
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="#6787c0" Offset="0"/>
                        <GradientStop Color="#879ec8" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <Grid>
                    <Canvas>
                        <Polygon Points="0,20,130,80,0,170" Fill="#879ec8"/>
                        <Polygon Points="100,400,200,370,180,470" Fill="#879ec8"/>
                        <Ellipse Margin="250 450 0 0" Width="40" Height="40" Fill="#879ec8"/>
                        <Ellipse Margin="50 400 0 0" Width="40" Height="40" Fill="#879ec8"/>

                    </Canvas>
                    <StackPanel VerticalAlignment="Bottom">
                        <TextBlock Text="ITM" Style="{StaticResource titleText}"/>
                        <TextBlock Text="Semiconductor" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0" FontWeight="Bold"/>
                        <TextBlock Text="AUTOMATION TEAM" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" >
                        <Label  FontWeight="Bold" Padding="0 20 0 0" Foreground="White" FontSize="27" HorizontalAlignment="Center">
                            <Label.Effect>
                                <DropShadowEffect Color="Black" Direction="320" ShadowDepth="5" Opacity="0.3"/>
                            </Label.Effect> System Menu
                        </Label>
                    </StackPanel>
                    <StackPanel Orientation="Vertical"  HorizontalAlignment="Center" Margin="0 100 0 0" >
                        <Button x:Name="btSetting1"  Height="60" Width="215" >
                            <StackPanel Orientation="Horizontal">
                                <!-- Add an Image control here with the source attribute pointing to your image file -->
                                <!--<Image Source="/Folder/PageUI/Icon/button off.png" VerticalAlignment="Center"  Margin="0 5 0 0" ></Image>-->
                                <TextBlock Padding="10" Text="Magazine"  Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                        <Button x:Name="btSetting2" Background="LightGreen" Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0 5 5 5" BorderThickness="0">
                            <StackPanel Orientation="Horizontal">

                                <!--<Image Source="/Folder/PageUI/Icon/button on.png" VerticalAlignment="Center"  Margin="0 5 0 0" ></Image>-->

                                <TextBlock Padding="10" Text="Tray CH1" Foreground="Black" Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>


                        <Button x:Name="btSetting3"  Height="60" Width="215" >
                            <StackPanel Orientation="Horizontal">
                                <!--Add an Image control here with the source attribute pointing to your image file-->
                                <!--<Image Source="/Folder/PageUI/Icon/button off.png" VerticalAlignment="Center"  Margin="0 5 0 0" ></Image>-->
                                <TextBlock Padding="10" Text="Tray CH2"  Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                        <Button x:Name="btSetting4"  Height="60" Width="215" >
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Padding="10" Text="Tray/Jig Setup "  Margin="10" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
        <Grid Grid.Column="0">
            <Grid.Resources>

                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="#dd222222" />
                    <Setter Property="Padding" Value="5,0" />
                    <Setter Property="VerticalContentAlignment" Value="Center" />
                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                    <Setter Property="FontSize" Value="20" />
                </Style>
                <Style TargetType="Button" BasedOn="{StaticResource ssButton}">
                    <Setter Property="FontSize" Value="14"/>
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="Margin" Value="9"/>
                    <Setter Property="Background" Value="White"/>
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="#6787c0"/>
                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="22" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Foreground" Value="Black"/>

                </Style>

            </Grid.Resources>
            <Grid Grid.Column="0" Margin="5,0,2,2" Name="gridLeft" Grid.ColumnSpan="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.7*"></RowDefinition>
                    <RowDefinition Height="2*"></RowDefinition>
                    <RowDefinition Height="3.5*"></RowDefinition>
                    <RowDefinition Height="5*"></RowDefinition>
                </Grid.RowDefinitions>

                <Border Grid.Row="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#bbbbbb" MinHeight="60">
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Background="Orange">
                            <Label Grid.Column="0" Foreground="#EEEEEE" FontSize="25" FontWeight="Bold"
                               Margin="20,0,0,0" VerticalContentAlignment="Center">
                                Base Pos. Manual Teaching: [Robot]
                            </Label>
                        </Grid>

                        <TextBlock Grid.Column="1" x:Name="tvLog1" FontSize="20"
                               HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                        <Button Grid.Column="2" x:Name="btSave1" Margin="5 5 5 5">
                            <StackPanel Orientation="Horizontal">
                                <Image Margin="5" Source="/Folder/PageUI/Icon/save.png" />
                                <TextBlock VerticalAlignment="Center" Margin="10,0,10,0"
             FontSize="15" FontWeight="Bold">Save</TextBlock>
                            </StackPanel>
                        </Button>


                    </Grid>

                    <!--<TextBlock Foreground="Orange" FontSize="24" Margin="20,0,0,0" VerticalAlignment="Center">
                    Base Teaching
                </TextBlock>-->
                </Border>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.4*"></ColumnDefinition>
                        <ColumnDefinition Width="0.3*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="0.2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" FontSize="13" FontWeight="Bold"  >Actual Position</Label>
                        <Grid Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="0.17*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="1" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Y-Axis</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="2" FontSize="12"  Visibility="Hidden" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Z-Axis</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="1" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Position Data</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="2" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Actual Position</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="3" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Diference Value</Label>
                            <Grid Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionDataX1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">350.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="1" Margin="2,2,2,2" Background="#222222" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionDataZ1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">-1.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="ActualPositionX1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">350.01</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="2" Margin="2,2,2,2" Background="#222222" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="ActualPositionZ1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">-1.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="DeferencePositionX1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay3}">-0.01</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="3" Margin="2,2,2,2" Background="#222222" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="DeferencePositionZ1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay3}">-0.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                                </StackPanel>
                            </Grid>
                        </Grid>

                    </Grid>
                    <Grid Grid.Column="1" Margin="10,0,0,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" FontSize="13" FontWeight="Bold"  >Motor Status</Label>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="0.13*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border>
                                <Grid Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="2*"></RowDefinition>
                                        <RowDefinition Height="1*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="1,1,1,1" Background="#BBBBBB">
                                        <Label FontSize="20" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold">Axis Y</Label>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Label Margin="1,1,1,1" Grid.Column="0" Style="{StaticResource LabelLime}">S</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="1" Style="{StaticResource LabelBlue}">H</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="2" Style="{StaticResource LabelBlack}">A</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="3" Style="{StaticResource LabelBlack}">P</Label>
                                        <Label Margin="1,1,1,1" Grid.Column="4" Style="{StaticResource LabelBlack}">N</Label>
                                    </Grid>
                                </Grid>
                            </Border>

                            <!--<Grid Grid.Column="1">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="2*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0" Margin="1,1,1,1" Background="#BBBBBB">
                                <Label FontSize="20" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold">Axis Z</Label>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Label Margin="1,1,1,1" Grid.Column="0" Style="{StaticResource LabelLime}">S</Label>
                                <Label Margin="1,1,1,1" Grid.Column="1" Style="{StaticResource LabelBlue}">H</Label>
                                <Label Margin="1,1,1,1" Grid.Column="2" Style="{StaticResource LabelBlack}">A</Label>
                                <Label Margin="1,1,1,1" Grid.Column="3" Style="{StaticResource LabelBlack}">P</Label>
                                <Label Margin="1,1,1,1" Grid.Column="4" Style="{StaticResource LabelBlack}">N</Label>
                            </Grid>
                        </Grid>-->
                        </Grid>

                    </Grid>
                    <Grid Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" FontSize="13" FontWeight="Bold"  >Monitor Data Robot</Label>
                        <Grid Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="1" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Axis-X</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Data Robot</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="1" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Current Position</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="2" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Current Speed</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="3" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Setting Speed</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="3" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Setting Speed</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="2" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Axis-Y</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="3" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Axis-Z</Label>
                            <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="4" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Axis-R</Label>

                            <Grid Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_X_CH1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">350.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtSpeedCurrentRobot_CH1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">350.01</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm/s</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <!--<TextBox Name="txtSetSpeedRobot1_CH1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay3}">-0.01</TextBox>-->
                                    <xctk:IntegerUpDown Value="100" Minimum="0" Maximum="100" Foreground="White" Background="#454343" x:Name="txtSetSpeedRobot1_CH1" Width="50" Margin="2,2,2,2" />
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">%</Label>
                                </StackPanel>
                            </Grid>

                            <Grid Grid.Column="2" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_Y_CH1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay1}">350.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="3" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_Z_CH1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">-350.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Column="4" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBox Name="txtPositionRobot_R_CH1" FontSize="18" FontFamily="Agency FB" Style="{StaticResource TextBoxDisplay2}">350.00</TextBox>
                                    <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="10">mm</Label>
                                </StackPanel>
                            </Grid>

                        </Grid>

                    </Grid>
                    <Grid Grid.Column="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.2*"></RowDefinition>
                            <RowDefinition Height="5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="1">
                            <Button Grid.Row="1" Margin="15,10,10,10" x:Name="btReset1">
                                <StackPanel Orientation="Vertical">
                                    <Image Height="60" Width="60" Source="/Folder/PageUI/Icon/alarm.png" Stretch="Fill"/>
                                    <TextBlock  VerticalAlignment="Center" Padding="20,0,0,0" FontSize="20" Foreground="Blue" FontFamily="Times New Roman (Headings)">Error Clear</TextBlock>
                                </StackPanel>
                            </Button>
                        </Grid>
                    </Grid>
                </Grid>
                <TabControl Name="TabControl1" Grid.Row="2">
                    <TabItem Height="30" Header="      Axis Y      ">
                        <Grid>
                            <Border Grid.Row="2" Margin="5,5,5,5" BorderBrush="Silver" Background="#FFFFFF" BorderThickness="1" CornerRadius="2,2,2,2">
                                <Grid Grid.Row="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*"></RowDefinition>
                                        <RowDefinition Height="1.5*"></RowDefinition>
                                        <RowDefinition Height="2*"></RowDefinition>

                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Margin="10,2,10,2" BorderThickness="1" BorderBrush="#222222">
                                        <Label Margin="2,2,2,2" BorderThickness="0" Background="Orange" FontWeight="Bold" Foreground="White" HorizontalContentAlignment="Center">Select the location you want to move(이동하려는 위치를 선택하십요)</Label>
                                    </Border>
                                    <Border Grid.Row="1" Margin="10,2,10,2" BorderThickness="1" Background="LightYellow" CornerRadius="2,2,2,2">
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                                <ColumnDefinition Width="0*"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Button Name="btnBendingPos" Grid.Column="1" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">Pick Pos.</Button>
                                            <Button Name="btnReadyPos" Grid.Column="2" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">Center Pos.</Button>
                                            <Button Name="btnQrPos" Grid.Column="3" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">Place Pos.</Button>
                                            <Button Name="btnQrScap" Grid.Column="4" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">Center Pos.2</Button>
                                            <!--<Button Name="btnMesScap" Grid.Column="5" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">5 Pos.</Button>-->
                                            <Button Name="btnMatingPos" Grid.Column="6" BorderBrush="#999999" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="#EEEEEE" Margin="10,2,10,2" Foreground="DarkBlue">Mating Pos.</Button>
                                        </Grid>
                                    </Border>
                                    <Grid Grid.Row="2" Margin="10,2,10,2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="2*"></ColumnDefinition>
                                            <ColumnDefinition Width="0*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Label Margin="1,1,1,1" Grid.Row="0" Grid.Column="0" Style="{StaticResource Labelteaching}">Position (mm)</Label>
